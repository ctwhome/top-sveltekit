{
  "name": "@top-sveltekit/postgres",
  "version": "1.0.0",
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/ctwhome/top-sveltekit.git",
    "directory": "postgres"
  },
  "scripts": {
    "dev": "vite dev",
    "build": "svelte-kit sync && vite build",
    "preview": "vite preview",
    "migrate": "source .env && DATABASE_URL=postgres://$DB_USER:$DB_PASSWORD@$DB_HOST:$DB_PORT/$DB_NAME node-pg-migrate",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "prettier --check . && eslint .",
    "format": "prettier --write .",
    "postbuild": "vite build --mode preview",
    "seed": "source .env && psql $DATABASE_URL -f seed-database/seed.sql",
    "release": "semantic-release"
  },
  "devDependencies": {
    "@auth/pg-adapter": "^1.10.0",
    "@iconify/json": "^2.2.386",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/github": "^11.0.6",
    "@sindresorhus/slugify": "^3.0.0",
    "@sveltejs/adapter-node": "^5.3.2",
    "@sveltejs/adapter-vercel": "^5.10.2",
    "@sveltejs/kit": "^2.42.2",
    "@sveltejs/vite-plugin-svelte": "^6.2.0",
    "@tailwindcss/container-queries": "^0.1.1",
    "@tailwindcss/forms": "^0.5.10",
    "@tailwindcss/typography": "^0.5.18",
    "@tailwindcss/vite": "^4.1.13",
    "@types/bcrypt": "^6.0.0",
    "@types/bcryptjs": "^2.4.6",
    "@types/eslint": "^9.6.1",
    "@types/node-pg-migrate": "^2.3.1",
    "@types/pg": "^8.15.5",
    "@types/sortablejs": "^1.15.8",
    "@typescript-eslint/eslint-plugin": "^8.44.0",
    "@typescript-eslint/parser": "^8.44.0",
    "@vcarl/remark-headings": "^0.1.0",
    "bcryptjs": "^3.0.2",
    "config": "^4.1.1",
    "daisyui": "^5.1.13",
    "dotenv": "^17.2.2",
    "dotenv-expand": "^12.0.3",
    "eslint": "^9.36.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-svelte": "^3.12.4",
    "fs-extra": "^11.3.2",
    "glob": "^11.0.3",
    "idb": "^8.0.3",
    "mdsvex": "^0.12.6",
    "mdsvex-relative-images": "^2.0.0",
    "node-pg-migrate": "^8.0.3",
    "pg": "^8.16.3",
    "postgres": "^3.4.7",
    "prettier": "^3.6.2",
    "prettier-plugin-svelte": "^3.4.0",
    "prettier-plugin-tailwindcss": "^0.6.14",
    "rehype-autolink-headings": "^7.1.0",
    "rehype-slug": "^6.0.0",
    "remark-external-links": "^9.0.1",
    "remark-reading-time": "^2.0.2",
    "remark-toc": "^9.0.0",
    "semantic-release": "^24.2.9",
    "svelte": "^5.39.3",
    "svelte-check": "^4.3.1",
    "svelte-preprocess-import-assets": "^1.1.0",
    "svelte-youtube-embed": "^0.4.6",
    "tailwindcss": "^4.1.13",
    "tslib": "^2.8.1",
    "typescript": "^5.9.2",
    "unist-util-visit": "^5.0.0",
