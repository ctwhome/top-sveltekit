    "vite-plugin-restart": "^1.0.0",
    "vite-plugin-static-copy": "^3.1.2",
    "vitest": "^3.2.4"
  },
  "type": "module",
  "dependencies": {
    "@auth/sveltekit": "^1.10.0",
    "ctw-kit": "^1.0.40",
    "date-fns": "^4.1.0",
    "eruda": "^3.4.3",
    "node-html-parser": "^7.0.1",
    "svelte-french-toast": "^1.2.0"
  },
  "overrides": {
    "estree-walker": "2.0.2"
  }
}
