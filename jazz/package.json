    "globals": "^15.11.0",
    "prettier": "^3.3.2",
    "prettier-plugin-svelte": "^3.2.6",
    "svelte": "^5.0.0",
    "svelte-check": "^4.0.0",
    "typescript": "~5.6.2",
    "typescript-eslint": "^8.0.0",
